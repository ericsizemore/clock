{
    "$schema": "https://raw.githubusercontent.com/infection/infection/0.28.1/resources/schema.json",
    "source": {
        "directories": [
            "src/"
        ],
        "excludes": [
            "tests/",
            "vendor/"
        ]
    },
    "mutators": {
        "@default": true
    },
    "logs": {
        "stryker": {
            "badge": "master"
        }
    },
    "tmpDir": "build/logs/infection",
    "testFramework": "phpunit"
}